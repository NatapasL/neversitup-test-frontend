{
  "plugins": ["@typescript-eslint"],
  "extends": ["next/core-web-vitals"],
  "rules": {
    "@typescript-eslint/explicit-function-return-type": [
      "warn",
      { "allowExpressions": false }
    ],
    "max-len": ["warn", { "code": 120, "ignoreStrings": true }]
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  }
}
